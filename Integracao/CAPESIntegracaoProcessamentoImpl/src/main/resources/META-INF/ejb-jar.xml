<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar id="EJBModule_CAPESIntegracaoProcessamentoImpl" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">

	<display-name>CAPESIntegracaoProcessamentoImpl</display-name>

	<interceptors>
		<interceptor>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.UsuarioInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.TransacaoBancariaInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.CacheInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.sicoob.capes.integracao.negocio.servicos.interceptors.CAPESIntegracaoInternacionalizacaoInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.TempoExecucaoInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
	</interceptors>

	<assembly-descriptor>
		<interceptor-binding>
			<ejb-name>*</ejb-name>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.UsuarioInterceptor</interceptor-class>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.TransacaoBancariaInterceptor</interceptor-class>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.CacheInterceptor</interceptor-class>
			<interceptor-class>br.com.sicoob.capes.integracao.negocio.servicos.interceptors.CAPESIntegracaoInternacionalizacaoInterceptor</interceptor-class>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.TempoExecucaoInterceptor</interceptor-class>
		</interceptor-binding>
	</assembly-descriptor>

</ejb-jar>