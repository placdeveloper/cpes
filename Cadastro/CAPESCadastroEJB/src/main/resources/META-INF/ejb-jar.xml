<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar id="EJBModule_CapesCadastroEJB" version="3.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd">

	<display-name>CapesCadastroEJB</display-name>

	<interceptors>
		<interceptor>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.UsuarioInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.TransacaoBancariaInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.CacheInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.CAPESCadastroInternacionalizacaoInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.TempoExecucaoInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.AutorizacaoCadastroInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.SistemaCooperativoInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.GrupoCompartilhamentoInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
		<interceptor>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.ReplicacaoInterceptor</interceptor-class>
			<around-invoke>
				<method-name>intercept</method-name>
			</around-invoke>
		</interceptor>
	</interceptors>

	<assembly-descriptor>
		<interceptor-binding>
			<ejb-name>*</ejb-name>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.UsuarioInterceptor</interceptor-class>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.TransacaoBancariaInterceptor</interceptor-class>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.CacheInterceptor</interceptor-class>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.CAPESCadastroInternacionalizacaoInterceptor</interceptor-class>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.SistemaCooperativoInterceptor</interceptor-class>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.GrupoCompartilhamentoInterceptor</interceptor-class>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.AutorizacaoCadastroInterceptor</interceptor-class>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.ReplicacaoInterceptor</interceptor-class>
			<interceptor-class>br.com.bancoob.negocio.servicos.interceptors.TempoExecucaoInterceptor</interceptor-class>
		</interceptor-binding>
		
		<interceptor-binding>
			<ejb-name>PessoaCompartilhamentoServicoEJB</ejb-name>
			<interceptor-class>br.com.sicoob.capes.cadastro.negocio.servicos.interceptors.InclusaoPessoaCompartilhamentoInterceptor</interceptor-class>
			<method>
				<method-name>incluir</method-name>
				<method-params>
					<method-param>br.com.sicoob.capes.negocio.entidades.vigente.PessoaCompartilhamento</method-param>
					<method-param>br.com.bancoob.persistencia.types.DateTimeDB</method-param>
					<method-param>br.com.sicoob.capes.negocio.entidades.Instituicao</method-param>
				</method-params>
			</method>
		</interceptor-binding>

	</assembly-descriptor>
</ejb-jar>